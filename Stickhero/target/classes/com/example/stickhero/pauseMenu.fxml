<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stickhero.PauseMenu">
   <ImageView fitHeight="66.0" fitWidth="66.0" layoutX="170.0" onMouseClicked="#onRestartButtonClick" pickOnBounds="true" preserveRatio="true">
      <Image url="@images/btnretry.png" />
   </ImageView>
   <ImageView fitHeight="66.0" fitWidth="66.0" onMouseClicked="#onHomeButtonClick" pickOnBounds="true" preserveRatio="true">
      <Image url="@assets/btnhome.png" />
   </ImageView>
   <ImageView fitHeight="66.0" fitWidth="66.0" layoutX="85.0" onMouseClicked="#onSaveButtonClick" pickOnBounds="true" preserveRatio="true">
      <Image url="@assets/save.png" />
   </ImageView>
   <Text fill="#14ff0c" layoutX="30.0" layoutY="-10.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Paused">
      <font>
         <Font size="48.0" />
      </font>
   </Text>
</AnchorPane>
